<div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat: -23.5505, lng: -46.633};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 10,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?<%=ENV["GOOGLE_MAP_KEY"]%>&callback=initMap">
    </script>

